{
    "manifest_version": 2,
    "name": "ViewWindows",
    "version": "2025.3.1",
    "browser_action": {},
    "permissions": [
        "tabs",
        "activeTab",
        "<all_urls>",
        "contextMenus"
    ],
    "commands": {
        "splitWindows": {
            "suggested_key": {
                "default": "Alt+S"
            },
            "description": "splitWindows"
        },
        "toOneWindow": {
            "suggested_key": {
                "default": "Alt+A"
            },
            "description": "toOneWindow"
        }
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}
