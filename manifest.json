{
    "manifest_version": 2,
    "name": "viewWindows",
    "version": "2023.3.17",
    "browser_action": {
        "default_icon": {
            "128": "icon128.png"
        }
    },
    "permissions": [
        "tabs",
        "activeTab",
        "<all_urls>",
        "contextMenus"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}
